<head>
    {{> head}}

</head>

<body>

    {{> header}}

    <div class="titulo-galeria">
        <h1 class="titulo-paginas">Galer√≠a</h1>
        <p class="cantidad-productos">20 productos</p>
    </div>

    <div class="contenedor-productos">
        {{#each productos}}
        <section class="producto">
            <img src="{{imagen}}" alt="" class="producto__imagen">
            <article class="producto__info">
            <p class="producto__categoria">{{categoria}}</p>
            <h4 class="producto__nombre">{{nombre}}</h4>
            <p class="producto__precio">${{precio}}</p>
            </article>

        </section>

        {{/each}}
    </div>
</body>

{{> footer}}